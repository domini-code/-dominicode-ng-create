{
  "name": "@dominicode/ng-create",
  "version": "1.0.0",
  "description": "CLI to generate Angular 21 projects with all modern tools",
  "type": "module",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "create-dominicode-ng": "bin/index.js"
  },
  "files": [
    "bin",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.d.ts.map",
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "start": "node bin/index.js"
  },
  "keywords": [
    "angular",
    "angular21",
    "cli",
    "generator",
    "scaffold",
    "boilerplate",
    "tailwind",
    "vitest",
    "jest",
    "eslint",
    "husky"
  ],
  "author": "Dominicode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dominicode/ng-create-cli.git"
  },
  "bugs": {
    "url": "https://github.com/dominicode/ng-create-cli/issues"
  },
  "homepage": "https://github.com/dominicode/ng-create-cli#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^10.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "typescript": "^5.9.2",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
